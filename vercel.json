{
  "version": 2,
  "builds": [
    {
      "src": "backend/main.py",
      "use": "@vercel/python"
    },
    {
      "src": "frontend/app.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/oauth/.*",
      "dest": "backend/main.py"
    },
    {
      "src": "/fetch_data",
      "dest": "backend/main.py"
    },
    {
      "src": "/youtube/.*",
      "dest": "backend/main.py"
    },
    {
      "src": "/.*",
      "dest": "frontend/app.py"
    }
  ]
}
